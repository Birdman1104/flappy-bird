{
  "compilerOptions": {
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "target": "esnext",
    "downlevelIteration": true,
    "skipLibCheck": true,
    "allowUmdGlobalAccess": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "baseUrl": "./"
  },
  "exclude": ["node_modules"]
}
